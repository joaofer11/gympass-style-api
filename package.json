    "@typescript-eslint/parser": "6.5.0",
    "eslint": "8.48.0",
    "npm-run-all": "4.1.5",
    "prisma": "5.2.0",
    "tsup": "7.2.0",
    "tsx": "3.12.7",
    "typescript": "5.2.2",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.34.3"
  },
  "dependencies": {
    "@fastify/jwt": "7.2.1",
